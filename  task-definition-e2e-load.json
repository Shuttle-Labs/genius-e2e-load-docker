{
  "family": "e2e-load-test",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "4096",
  "memory": "8192",
  "taskRoleArn": "arn:aws:iam::445914872260:role/genius_ECSExec_Role",
  "executionRoleArn": "arn:aws:iam::445914872260:role/genius_ECSExec_Role",
  "containerDefinitions": [
    {
      "name": "playwright-load",
      "image": "445914872260.dkr.ecr.us-east-2.amazonaws.com/e2e_load_test:latest",
      "environment": [
        {
          "name": "BASE_URL",
          "value": "https://www.tradegenius.com"
        },
        {
          "name": "METAMASK_PASSWORD",
          "value": "Tester@1234"
        },
        {
          "name": "SEED_PHRASE",
          "value": "brick jeans notice danger fatigue judge turtle retire miss hold office sauce"
        }
      ],
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "mode": "non-blocking",
          "awslogs-create-group": "true",
          "awslogs-group": "/ecs/e2e-load-test",
          "max-buffer-size": "25m",
          "awslogs-region": "us-east-2",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}
